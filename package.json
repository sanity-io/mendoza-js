{
  "name": "mendoza",
  "version": "2.1.1",
  "description": "Mendoza, differ for structured documents",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es6 --outDir lib/esm",
    "lint": "echo todo",
    "coverage": "npm test",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "prepublishOnly": "npm run build",
    "clean": "rimraf lib",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@sanity/semantic-release-preset": "^4.1.2",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.1",
    "ls-engines": "^0.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^21.0.7",
    "ts-jest": "26.4.1",
    "ts-node": "^8.6.2",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/mendoza-js.git"
  },
  "keywords": [
    "mendoza",
    "diff"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/mendoza-js/issues"
  },
  "homepage": "https://www.sanity.io/",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
